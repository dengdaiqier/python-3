class Animal(object):
	pass
#大类

class Mammal(Animal):
	def arg(self):
		print('arg方法函数')
	def run(self):
		print('run`')		
class Bird(Animal):
	pass
#各种动物

class Runnable(object):
	def run(self):
		print('Running')
class Flyable(object):
	def fly(self):
		print('Flying')

class Dog(Mammal,Runnable):
	pass
class Bat(Mammal,Flyable):
	pass
class Parrot(Bird):
	pass
class Ostrich(Bird):
	pass

s=Dog()
s.run()

s.arg()
print(type(Dog))
x=Bat()
x.fly()

if __name__ == '__main__':
	print(Dog.__mro__)
	#__mro__表示显示类的继承先后顺序
