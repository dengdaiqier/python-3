# 定义函数
#-*- coding:utf-8 -*-
def my_new(x):
	if isinstance(x,(int,float)):
		raise TypeError('cuol')
	elif x>0:
		print('你输入的值大于0')
	else:
		print('你输入的值小于0')		
my_new(10)

